<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="TmecItemSong">
        <attr name="tmec_item_song_number" format="string" />
        <attr name="tmec_item_song_score" format="string" />
        <attr name="tmec_item_song_singer" format="string" />
        <attr name="tmec_item_song_songname" format="string" />
        <attr name="tmec_icon_left_drawable" format="reference" />
        <attr name="tmec_icon_right_drawable" format="reference" />
        <attr name="tmec_icon_left_night_drawable" format="reference" />
        <attr name="tmec_icon_right_night_drawable" format="reference" />
        <attr name="tmec_item_song_show_left_icon" format="boolean" />
        <attr name="tmec_item_song_show_bottom_line" format="boolean" />
    </declare-styleable>

    <declare-styleable name="TmecItemSongImageRight">
        <attr name="showCheckBox" format="boolean" />
        <attr name="song_name" format="string" />
        <attr name="singer_name" format="string" />
        <attr name="song_score" format="string" />
        <attr name="right_image" format="reference" />
    </declare-styleable>

    <declare-styleable name="TmecItemSongImageLeft">
        <attr name="image_left_song_name" format="string" />
        <attr name="image_left_singer_name" format="string" />
        <attr name="image_left_album_image" format="reference" />
        <attr name="image_left_song_rank">
            <enum name="rank_first" value="1"/>
            <enum name="rank_second" value="2"/>
            <enum name="rank_third" value="3"/>
        </attr>
    </declare-styleable>
</resources>