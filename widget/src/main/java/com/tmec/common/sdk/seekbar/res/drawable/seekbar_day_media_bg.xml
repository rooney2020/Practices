<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:id="@android:id/background"
        android:height="@dimen/dimens_seekbar_8dp"
        android:gravity="center_vertical">
        <shape android:shape="rectangle">
            <stroke android:width="@dimen/dimens_seekbar_stroke_1dp"
                android:color="@color/tmec_seekbar_media_stroke_bg"/>
            <solid android:color="@color/tmec_seekbar_media_solid_bg" />
            <corners android:radius="@dimen/dimens_seekbar_2dp" />
        </shape>
    </item>

    <item
        android:id="@android:id/progress"
        android:gravity="center_vertical">
        <clip android:drawable="@drawable/tmec_seekbar_media_bg"/>
    </item>
</layer-list>