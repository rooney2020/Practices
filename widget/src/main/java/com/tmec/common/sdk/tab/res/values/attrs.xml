<resources>
    <style name="DefaultTabItem">
        <item name="android:height">@dimen/tab_item_height</item>
        <item name="android:width">@dimen/tab_item_width</item>
        <item name="android:minHeight">@dimen/tab_item_height</item>
        <item name="android:minWidth">@dimen/tab_item_width</item>
        <item name="hryt_checkedItemColor">@null</item>
        <item name="hryt_tabItemNormalBackgroundTint">@color/hryt_tabItem_vertical_bg</item>
        <item name="hryt_tabItemTopBackgroundTint">@color/hryt_tabItem_top_bg</item>
        <item name="hryt_tabItemCheckBackgroundTint">@color/hrty_tabItem_check_bg</item>
        <item name="hryt_topCheckDrawable">@drawable/img_tabitem_top_checked1</item>
        <!--check color-->
        <item name="hryt_tabItemCheckTint">@color/hryt_main_color_normal</item>
        <item name="hryt_tabItemItemDrawable">@null</item>
        <item name="hryt_tabItemTopBgDrawable">@drawable/img_tabitem_top_bg</item>
        <item name="hryt_tabItemVerticalItemDrawable">@drawable/img_tabitem_side_checked2</item>
        <item name="hryt_tabItemTopCheckTint">@color/hrty_tabItem_check_bg</item>

        <!--item whole text's color-->
        <item name="hryt_tabItemTextColor">@color/hryt_assist_content_color</item>

        <!--item select text's color-->
        <item name="hryt_tabItemSelectedTextColor">@color/hryt_main_content_color</item>
    </style>


    <declare-styleable name="HHTabItemLayout">
        <!--set view check item drawable-->
        <attr name="hryt_checkedItemColor" format="reference" />
        <attr name="hryt_topCheckDrawable" format="reference" />
        <attr name="hryt_tabItemTopBgDrawable" format="reference" />
        <!--tint-->
        <attr name="hryt_tabItemNormalBackgroundTint" format="reference" />
        <attr name="hryt_tabItemCheckBackgroundTint" format="reference" />
        <attr name="hryt_tabItemTopCheckTint" format="reference" />
        <attr name="hryt_tabItemTopBackgroundTint" format="reference" />
        <attr name="hryt_tabItemCheckTint" format="reference" />
        <attr name="hryt_tabItemVerticalItemDrawable" format="reference" />
        <!--set view item drawable -->
        <attr name="hryt_tabItemItemDrawable" format="reference" />
        <!--item radius-->
        <attr name="hryt_tabItemRadius" format="dimension|reference" />

        <!--item whole background's color-->
        <attr name="hryt_tabItemOuterColor" format="color" />

        <!--item item background's color-->
        <attr name="hryt_tabItemItemColor" format="color" />

        <!--item whole text's color-->
        <attr name="hryt_tabItemTextColor" format="color" />

        <!--item whole text's disable color-->
        <attr name="hryt_tabItemDisableTextColor" format="color" />

        <!--item select text's color-->
        <attr name="hryt_tabItemSelectedTextColor" format="color" />

        <!--item whole text's size-->
        <attr name="hryt_tabItemTextSize" format="dimension|reference" />

        <!--item select text's size-->
        <attr name="hryt_tabItemSelectTextSize" format="dimension|reference" />

        <!--add item -->
        <attr name="hryt_tabItemAddItems" format="reference" />

        <!--set select item -->
        <attr name="hryt_tabItemSelectedItem" format="integer" />

        <!--set view can abable -->
        <attr name="hryt_tabItemEnabled" format="boolean" />

        <!--set view can scroll -->
        <attr name="hryt_tabItemScrollEnable" format="boolean" />

        <!--set view bg drawable -->
        <attr name="hryt_tabItemBgDrawable" format="reference" />

        <!--set view mode horizontal or vertical -->
        <attr name="hryt_tabItemOrientation" format="enum">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>

        <!--set child type -->
        <attr name="hryt_tabItemChildEnable" format="boolean" />
        <!--set view mode round or circle -->
        <attr name="hryt_tabItemMode" format="enum">
            <enum name="Round" value="0" />
            <enum name="Circle" value="1" />
        </attr>

        <!--item padding left and right-->
        <attr name="hryt_tabItemMarginLeft" format="dimension|reference" />

        <attr name="hryt_tabItemRedPointPosition" format="integer" />
        <attr name="hryt_tabItemRedPointShow" format="boolean" />
        <!--item padding top and bottom-->
        <attr name="hryt_tabItemMarginTop" format="dimension|reference" />

        <attr name="hryt_tabItemOnClickBeforeAnimation" format="boolean" />


        <!--set view can scroll -->
        <attr name="hryt_tabItemDefaultBitmap" format="boolean" />

        <!--set item text and image padding -->
        <attr name="hryt_tabItemTextAndImagePadding" format="dimension" />

    </declare-styleable>

    <declare-styleable name="HHSound">
        <attr name="hryt_soundEffect" format="enum">
            <!--      hh sound effect type-->
            <enum name="FX_KEY_NORMAL_REAL_BTN" value="0" />
            <enum name="FX_KEY_NORMAL_ICON_BTN" value="0" />
            <enum name="FX_KEY_NORMAL_GHOST_BTN" value="0" />
            <enum name="FX_KEY_NORMAL_FLAT_BTN" value="0" />
            <enum name="FX_KEY_NORMAL_TOGGLE_BTN" value="0" />
            <enum name="FX_KEY_NORMAL_DIALOG" value="0" />
            <enum name="FX_KEY_NORMAL_POP" value="0" />
            <enum name="FX_KEY_NORMAL_TAB" value="0" />
            <enum name="FX_KEY_NORMAL_SEGMENT" value="0" />
            <enum name="FX_KEY_NORMAL_PICK" value="0" />
            <enum name="FX_KEY_NORMAL_EDIT" value="0" />
            <enum name="FX_KEY_NORMAL_SWIP_FRESH_LY" value="0" />
            <enum name="FX_KEY_NORMAL_CHECK_BOX" value="0" />
            <enum name="FX_KEY_NORMAL_SWITCH" value="0" />
            <!--      android view sound effect type-->
            <enum name="FX_KEY_CLICK" value="0" />
            <enum name="FX_KEY_TV" value="0" />
            <enum name="FX_KEY_IMAGE" value="0" />
            <enum name="FX_KEY_CHECK_BOX" value="0" />
            <enum name="FX_KEY_SWITCH" value="0" />
            <enum name="FX_KEY_IMG_BTN" value="0" />
            <enum name="FX_KEY_RADIO_BTN" value="0" />
            <enum name="FX_KEY_3D_BTN" value="0" />
            <enum name="FX_KEY_CUSTOM_VIEW" value="0" />
            <!--      third sound  effect type-->
            <enum name="FX_SFK_SUCCESS" value="0" />
            <enum name="FX_SFK_FAIL" value="0" />
            <enum name="FX_SFK_CALLING_ONE" value="0" />
            <enum name="FX_SFK_CALLING_TWO" value="0" />
            <enum name="FX_SFK_CALLING_THREE" value="0" />
            <enum name="FX_SFK_CALLING_FOUR" value="0" />
            <enum name="FX_SFK_CALLING_FIVE" value="0" />
            <enum name="FX_SFK_CALLING_SIX" value="0" />
            <enum name="FX_SFK_CALLING_SEVEN" value="0" />
            <enum name="FX_SFK_CALLING_EIGHT" value="0" />
            <enum name="FX_SFK_CALLING_NINE" value="0" />
            <enum name="FX_SFK_CALLING_TEN" value="0" />
            <enum name="FX_SFK_STAR" value="0" />
            <enum name="FX_SFK_NUMBER_SIGN" value="0" />
            <enum name="FX_SFK_CONFIRM" value="0" />
            <enum name="FX_SFK_DELET" value="0" />
            <enum name="FX_SFK_AML" value="0" />
            <enum name="FX_SFK_CALLING_INCOMMING" value="0" />
            <enum name="FX_SFK_CAMERA" value="0" />
            <enum name="FX_SFK_IROBOT_ONE" value="0" />
            <enum name="FX_SFK_IROBOT_TWO" value="0" />
            <enum name="FX_SFK_SCENE_TWO" value="0" />
            <enum name="FX_SFK_SCENE_ONE" value="0" />
            <enum name="FX_SFK_IROBOT_THREE" value="0" />
            <enum name="FX_SFK_SYS_WAR_H" value="0" />
            <enum name="FX_SFK_SYS_WAR_M" value="0" />
            <enum name="FX_SFK_SYS_WAR_L" value="0" />
            <enum name="FX_SFK_VOICE_RECOG_ONE" value="0" />
            <enum name="FX_SFK_VOICE_RECOG_TWO" value="0" />
            <enum name="FX_SFK_WAKEUP" value="0" />
            <enum name="FX_KEY_NOTIFICATION" value="0" />
        </attr>
    </declare-styleable>

    <declare-styleable name="HHScrollTabItemLayout">
        <!--set view can abable -->
        <attr name="hryt_scrollTabItemEnabled" format="boolean" />

        <!--set view mode horizontal or vertical -->
        <attr name="hryt_scrollTabItemOrientation" format="enum">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>

        <attr name="hryt_scrollTabItemItemType" format="enum">
            <enum name="text" value="0" />
            <enum name="icon_text" value="1" />
        </attr>

        <!--set child type -->
        <attr name="hryt_scrollTabItemChildEnable" format="boolean" />
        <attr name="hryt_scrollTabItemItemWidth" format="dimension" />
        <!--item whole text's size-->
        <attr name="hryt_scrollTabItemTextSize" format="dimension|reference" />

        <!--item select text's size-->
        <attr name="hryt_scrollTabItemSelectTextSize" format="dimension|reference" />

        <!--item padding left and right-->
        <attr name="hryt_scrollTabItemMarginLeft" format="dimension|reference" />

        <attr name="hryt_scrollTabItemRedPointPosition" format="integer" />
        <attr name="hryt_scrollTabItemRedPointShow" format="boolean" />

        <!--item padding top and bottom-->
        <attr name="hryt_scrollTabItemMarginTop" format="dimension|reference" />

        <attr name="hryt_scrollTabItemOverScrollMode">
            <!-- Always show over-scroll effects, even if the content fits entirely
                 within the available space. -->
            <enum name="always" value="0" />
            <!-- Only show over-scroll effects if the content is large
                 enough to meaningfully scroll. -->
            <enum name="ifContentScrolls" value="1" />
            <!-- Never show over-scroll effects. -->
            <enum name="never" value="2" />
        </attr>
    </declare-styleable>

    <style name="HHVerticalScrollbarStyle">
        <item name="android:scrollbars">vertical</item>
        <item name="android:scrollbarStyle">outsideOverlay</item>
        <item name="android:scrollbarThumbVertical">@drawable/scroll_progress_shape</item>
        <item name="android:scrollbarTrackVertical">@drawable/scroll_bg_shape</item>
        <item name="android:overScrollMode">never</item>
    </style>

    <style name="HHHorizontalScrollbarStyle">
        <item name="android:scrollbars">horizontal</item>
        <item name="android:overScrollMode">never</item>
        <item name="android:scrollbarThumbHorizontal">@drawable/scroll_progress_shape</item>
        <item name="android:scrollbarTrackHorizontal">@drawable/scroll_bg_shape</item>
    </style>
</resources>
